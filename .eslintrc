{
  "parser": "babel-eslint",
  "extends": [
    "react-app"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": ["warn", { "vars": "all" }]
  }
}
